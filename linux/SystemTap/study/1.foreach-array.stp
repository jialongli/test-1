global odds, evens

probe begin {
	# "no" and "ne" are local integers
	for (i = 0; i < 10; i++) {
		if (i % 2) odds [no++] = i
		else evens [ne++] = i
	}
	delete odds[2]
	delete evens[3]
	exit()
}
probe end {
	foreach (x+ in odds)
		printf ("odds[%d] = %d\n", x, odds[x])
	foreach (x in evens-)
		printf ("evens[%d] = %d\n", x, evens[x])
}
